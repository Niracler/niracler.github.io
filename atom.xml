    <link href="https://www.niracler.com/2019/10/13/%E9%9D%A2%E8%AF%95%E9%A2%98-Python%E5%9F%BA%E7%A1%80-%E4%B8%80/"/>
    <id>https://www.niracler.com/2019/10/13/%E9%9D%A2%E8%AF%95%E9%A2%98-Python%E5%9F%BA%E7%A1%80-%E4%B8%80/</id>
    <published>2019-10-13T13:34:44.000Z</published>
